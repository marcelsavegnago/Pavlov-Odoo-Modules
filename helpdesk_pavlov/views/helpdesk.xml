<odoo>

  <record model="ir.ui.view" id="helpdesk_ticket_form">
  <!--  <field name="name">partner.bandwidth_change</field> -->
    <field name="model">helpdesk.ticket</field>
    <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_form"/>
    <field name="arch" type="xml">
      <data>
          <xpath expr="//field[@name='priority']" position="before">
            <field name="origin" string="Origin" widget="selection"/>
            <field name="severity" string="Severity" widget="selection"/>
          </xpath>
      </data>
    </field>
  </record>

</odoo>
