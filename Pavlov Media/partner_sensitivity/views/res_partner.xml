<odoo>
  <record id="view_partner_form" model="ir.ui.view">
    <field name="name">partner.sensitivity.form</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_partner_form"/>
    <field name="arch" type="xml">
      <field name="lang" position="after">
        <field name="sensitivity_id"
               options="{'no_create': True, 'no_open': True}"/>
      </field>
    </field>
  </record>

  <record id="view_res_partner_filter" model="ir.ui.view">
    <field name="name">partner.sensitivity.filter</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_res_partner_filter"/>
    <field name="arch" type="xml">
      <filter name="group_country" position="after">
        <filter name="group_sensativity"
                string="Sensativity Level"
                context="{'group_by': 'sensitivity_id'}"/>
        </filter>
    </field>
  </record>
</odoo>
