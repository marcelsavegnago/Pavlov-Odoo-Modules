<odoo>
  <data noupdate="1">
    <record id="fsm_building_style_cottage" model="fsm.building_style">
      <field name="name">Cottage</field>
      <field name="description">A cottage is characterized by buildings that are 1-2 units.</field>
      <field name="owner_sequence">0</field>
    </record>
    <record id="fsm_building_style_garden" model="fsm.building_style">
      <field name="name">Garden</field>
      <field name="description">A garden apartment complex is characterized by a cluster of low-rise buildings, usually 1-3 floors high..</field>
      <field name="owner_sequence">1</field>
    </record>
    <record id="fsm_building_style_midrise" model="fsm.building_style">
      <field name="name">Mid-Rise</field>
      <field name="description">A building or buildings with 4-8 floors.</field>
      <field name="owner_sequence">2</field>
    </record>
    <record id="fsm_building_style_hirise" model="fsm.building_style">
      <field name="name">Hi-Rise</field>
      <field name="description">A building or buildings that have more than 9 floors.</field>
      <field name="owner_sequence">3</field>
    </record>
    <record id="fsm_building_style_house" model="fsm.building_style">
      <field name="name">House</field>
      <field name="description">Residential house.</field>
      <field name="owner_sequence">4</field>
    </record>
  </data>
</odoo>
