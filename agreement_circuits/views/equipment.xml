<odoo>

  <record model="ir.ui.view" id="maintenance_equipment_form">
    <field name="model">maintenance.equipment</field>
    <field name="inherit_id" ref="maintenance.hr_equipment_view_form"/>
    <field name="arch" type="xml">
      <data>
          <xpath expr="//field[@name='maintenance_team_id']" position="before">
            <field name="customer" string="Customer"/>
            <field name="entitlement" string="Entitlement"/>
            <field name="product" string="Product"/>
          </xpath>
      </data>
    </field>
  </record>

</odoo>
