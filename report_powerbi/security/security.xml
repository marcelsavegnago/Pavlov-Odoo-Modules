<odoo>
  <!-- This holds the security groups -->
  <record model="ir.module.category" id="module_powerbi_category">
    <field name="name">PowerBI</field>
    <field name="description">User access level for PowerBI Server</field>
    <field name="sequence">4</field>
  </record>

  <record id="powerbi_group_user" model="res.groups">
    <field name="name">User</field>
    <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    <field name="category_id" ref="module_powerbi_category"/>
   </record>

   <record id="powerbi_group_manager" model="res.groups">
     <field name="name">Manager</field>
     <field name="implied_ids" eval="[(4, ref('powerbi_group_user'))]"/>
     <field name="category_id" ref="module_powerbi_category"/>
   </record>
</odoo>
