<odoo>
  <data noupdate="1">

    <record id="fsm_building_type_garden" model="fsm.building_type">
      <field name="name">Garden</field>
      <field name="description">A garden apartment complex is characterized by a cluster of low-rise buildings, usually no more than two or three stories high (although more modern complexes often rise to 4-5 floors), on a single piece of property.</field>
      <field name="owner_sequence">0</field>
    </record>

    <record id="fsm_building_type_hirise" model="fsm.building_type">
      <field name="name">Hi-Rise</field>
      <field name="description">A building or buildings that have more than 10 stories.</field>
      <field name="owner_sequence">1</field>
    </record>

    <record id="fsm_building_type_midrise" model="fsm.building_type">
      <field name="name">Mid-Rise</field>
      <field name="description">A building or buildings that have more than 4 stories but no more than 10 stories.</field>
      <field name="owner_sequence">2</field>
    </record>

    <record id="fsm_building_type_house" model="fsm.building_type">
      <field name="name">House</field>
      <field name="description">Residential house.</field>
      <field name="owner_sequence">3</field>
    </record>

    <record id="fsm_building_type_other" model="fsm.building_type">
      <field name="name">Other</field>
      <field name="description">This category is only to be used in the event that all other categories do not fit.</field>
      <field name="owner_sequence">4</field>
    </record>

  </data>
</odoo>
